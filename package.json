{
  "name": "luna-proto-files",
  "version": "0.0.2",
  "description": "Proto files for GRPC in luna system",
  "main": "index.js",
  "scripts": {
    "build-proto-types": "node ./node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js ./data_requests.proto --outDir=./build --longs=String --enums=String --oneofs --grpcLib=@grpc/grpc-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KirillBuiko/luna-proto-files.git"
  },
  "keywords": [
    "luna",
    "grpc",
    "proto"
  ],
  "author": "Kirill Buyko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KirillBuiko/luna-proto-files/issues"
  },
  "homepage": "https://github.com/KirillBuiko/luna-proto-files#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.12",
    "@grpc/proto-loader": "^0.7.10"
  }
}
